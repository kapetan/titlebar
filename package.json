{
  "name": "titlebar-react",
  "version": "1.3.0",
  "description": "Emulate OS X window title bar using React.js",
  "main": "index.js",
  "scripts": {
    "demo-watch": "wzrd index.js:demo/index.js -- -s titlebar",
    "demo-build": "browserify index.js -o demo/index.js -s titlebar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tgdn/titlebar-react"
  },
  "keywords": [
    "osx",
    "window",
    "titlebar",
    "stoplight",
    "react",
    "reactjs",
    "react.js"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgdn/titlebar-react/issues"
  },
  "homepage": "https://github.com/tgdn/titlebar-react",
  "dependencies": {
    "brfs": "^1.4.0",
    "classnames": "^2.1.3",
    "defaultcss": "^1.1.1",
    "dombo": "^3.2.0",
    "domify": "^1.3.3",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "wzrd": "^1.2.1"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  }
}
