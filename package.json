    "titlebar",
    "stoplight"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapetan/titlebar/issues"
  },
  "homepage": "https://github.com/kapetan/titlebar",
  "dependencies": {
    "brfs": "^1.4.0",
    "defaultcss": "^1.1.1",
    "dombo": "^3.2.0",
    "domify": "^1.3.3"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "wzrd": "^1.2.1"
  },
  "browserify": {
